{
  "name": "backend",
  "version": "1.0.0",
  "description": "GAN Integrity challenge backend",
  "main": "app.js",
  "scripts": {
    "complexity": "cr src",
    "lint:fix": "eslint --cache --fix",
    "lint:verify": "eslint .",
    "start": "nodemon app.js",
    "prettier:fix": "prettier --write *",
    "prettier:verify": "prettier --check \"*.{js,css,md,yml,json}\"",
    "start:docker": "docker compose up --build",
    "test": "jest",
    "test:api": "npm run build:postman && newman run test/mock/postman_collection.json -r cli,json",
    "test:api:watch": "chokidar '*.js' 'src/*.js' 'test/mock/postman_collection.json' --initial -c 'npm run test:api'",
    "test:api:unit": "jest  --forceExit --testPathPattern='.*/test/api/.*.test.js'",
    "test:api:unit:watch": "npm run test:api:unit -- --watch",
    "test:bdd": "jest ./features",
    "test:bdd:watch": "npm run test:bdd -- --watch",
    "test:security": "snyk test",
    "test:security:dev": "npm run test:security -- --dev",
    "test:unit": "jest ./test --testPathIgnorePatterns test/api/*",
    "devtest": "jest ./test --testPathIgnorePatterns test/api/* --detectOpenHandles --runInBand",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:coverage": "npm test -- --coverage"
  },
  "author": "Carlo Tr√∂nnberg",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.4",
    "body-parser": "^1.19.0",
    "complexity-report": "^2.0.0-alpha",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.7.0",
    "helmet": "^5.0.1",
    "json-stream-stringify": "^2.0.3",
    "JSONStream": "^1.3.5",
    "mongodb": "^3.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "jest": "^27.5.1",
    "jest-config": "^27.5.1",
    "jest-cucumber-fusion": "^0.8.1",
    "jest-pact": "^0.9.3",
    "supertest": "^6.2.2",
    "prettier": "^3.2.5",
    "lint-staged": "^12.5.0"
  }
}
